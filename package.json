{
  "name": "feedbacker-app",
  "version": "1.0.0",
  "private": true,
  "author": "andersonbosa <andersonbosa0@gmail.com>",
  "license": "MIT",
  "repository": "git@github.com:andersonbosa/feedbacker-app.git",
  "type": "commonjs",
  "scripts": {
    "dev": "lerna run dev --stream",
    "prd": "lerna run prd --stream",
    "release": "lerna publish",
    "bootstrap": "lerna bootstrap --nohoist",
    "clean": "lerna clean -y && rm -rf node_modules package-lock.json yarn.lock",
    "reboot": "yarn run clean && yarn && yarn run bootstrap"
  },
  "dependencies": {
    "feedbacker-api": "file:packages/feedbacker-api",
    "feedbacker-dashboard": "file:packages/feedbacker-dashboard"
  },
  "devDependencies": {
    "lerna": "^4.0.0"
  },
  "workspaces": [
    "packages/*"
  ],
  "engines": {
    "node": ">= 14.x.x"
  }
}
